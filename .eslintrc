{
  "parserOptions": {
    "ecmaVersion": 9,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "rules": {
    "no-useless-escape": "off",
    "no-console": "off",
    "no-undef": "off",
    "comma-dangle" : 0,
    "indent": [
      "error",
      4
    ]
  },
  "parser": "babel-eslint",
  "settings": {
    "react": {
      "pragma": "React",
      "version": "16.13.0"
    }
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
